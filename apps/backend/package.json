{
  "name": "@boilerplate/backend",
  "version": "1.0.0",
  "description": "Backend API with Fastify and Prisma",
  "main": "dist/server.js",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest unit",
    "test:integration": "jest integration --runInBand",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "clean": "rm -rf dist coverage tsconfig.tsbuildinfo .turbo"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.908.0",
    "@fastify/compress": "^8.1.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/jwt": "^10.0.0",
    "@fastify/multipart": "^9.2.1",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.5.2",
    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/client": "^6.16.3",
    "@sentry/node": "^10.19.0",
    "@sentry/profiling-node": "^10.19.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.6.2",
    "@types/node-cron": "^3.0.11",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.61.0",
    "cheerio": "^1.1.2",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.1",
    "ioredis": "^5.8.1",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.2.1",
    "openai": "^6.6.0",
    "pdf-parse": "^2.4.5",
    "pino": "^10.0.0",
    "prisma": "^6.16.3",
    "prom-client": "^15.1.3",
    "redlock": "^5.0.0-beta.2",
    "resend": "^6.1.2",
    "rss-parser": "^3.13.0",
    "stripe": "^19.1.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "zod": "^4.1.11"
  },
  "license": "MIT",
  "devDependencies": {
    "@boilerplate/eslint-config": "*",
    "@boilerplate/tsconfig": "*",
    "@eslint/js": "^9.37.0",
    "@types/jest": "^30.0.0",
    "@types/pdf-parse": "^1.1.5",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "jest-mock-extended": "^4.0.0",
    "nodemon": "^3.1.10",
    "pino-pretty": "^13.1.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "tsx": "^4.20.6",
    "typescript-eslint": "^8.45.0"
  }
}
